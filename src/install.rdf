<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <em:name>Webtest Recorder</em:name>
    <em:id>@extension.id@</em:id>
    <em:version>@versionNumber@</em:version>
    <em:type>2</em:type>
    <em:description>Recording tool to help creating test script for Webtest</em:description>
    <em:creator>Marc Guillemot</em:creator>
    <em:contributor>coming soon...</em:contributor>
    <em:homepageURL>http://webtestrecorder.canoo.com/</em:homepageURL>
    <!--em:optionsURL>chrome://webtestrecorder/content/optionsDialog.xul</em:optionsURL -->
	<em:updateURL>@updateUrl@</em:updateURL>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>19.*</em:maxVersion>
      </Description>
    </em:targetApplication> 
	<em:iconURL>chrome://webtestrecorder/content/wtr.png</em:iconURL>
    <em:file>
      <Description about="urn:mozilla:extension:file:webtestrecorder.jar">
        <em:package>content/</em:package>
      </Description>
    </em:file>

  </Description>

</RDF>
